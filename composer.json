{
    "name": "rhymedigital/contao_rhyme_salesforce",
    "description": "A Contao CMS extension that integrates with Salesforce",
    "keywords": [
    	"contao",
    	"salesforce",
    	"crm",
    	"force.com"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Rhyme Digital",
            "homepage": "http://rhyme.digital"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.3.0,<4.0.0-dev",
        "contao-community-alliance/composer-installer": "*",
        "terminal42/contao-namespace-class-loader":"~1.0",
        "phpforce/soap-client": "dev-partner",
        "doctrine/collections": ">=1.3"
    },
    "autoload": {
        "psr-0": {
            "Rhyme\\": [
                "library/"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/salesforce"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}